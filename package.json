{
  "name": "dm-toolkit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "nodemon --watch src/server --ext ts --exec 'tsx ./src/server/index.ts'",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:create": "db-migrate db:create",
    "db:up": "db-migrate up",
    "db:down": "db-migrate down",
    "db:reset": "db-migrate reset"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "db-migrate": "^0.11.14",
    "db-migrate-mysql": "^3.0.0",
    "express": "^4.19.2",
    "mysql": "^2.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.0.3",
    "@eslint/js": "^9.4.0",
    "@types/express": "^4.17.21",
    "@types/mysql": "^2.15.26",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.3.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.19.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.12.0",
    "vite": "^5.2.0"
  }
}
